<%= error_messages_for 'query' %>
<% if @query.valid? %>
		<% if @issues.empty? %>
				<p class="nodata"><%= l(:label_no_data) %></p>
		<% else %>
				<%= render :partial => 'list', :locals => {:issues => @issues, :query => @query} %>
				<p class="pagination"><%= pagination_links_full @issue_pages, @issue_count %></p>
		<% end %>
<% end %>
